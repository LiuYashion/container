{
  "extends": "airbnb-base",
  "plugins":[
    "react"
  ],
  "ecmaFeatures":{
    "jsx":true,
    "modules":true
  },
  "parser":"babel-eslint",
  "env":{
    "browser":true,
    "node":true,
    "es6":true
  },
  "rules":{
    "strict":0,
    "valid-jsdoc":2,
    "react/jsx-uses-react":2,
    "react/jsx-uses-vars":2,
    "react/react-in-jsx-scope":2
  }
}